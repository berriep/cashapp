{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "RabobankBAI": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "RabobankBAI": {
    "DefaultClient": "default",
    "TokenStorageBasePath": "tokens",
    "ConfigurationBasePath": "config/clients",
    "CertificateBasePath": "certificates"
  }
}