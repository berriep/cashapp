        }
      }
    }
  },
  "securityDefinitions": {
    "oauth2 access code flow": {
      "description": "NOTE Please use the Authorization and Token URL as provided by the oAuth2 Services",
      "type": "oauth2",
      "authorizationUrl": "https://oauth.rabobank.nl/openapi/oauth2-premium/authorize",
      "tokenUrl": "https://oauth.rabobank.nl/openapi/oauth2-premium/token",
      "flow": "accessCode",
      "scopes": {
        "bai.accountinformation.read": "Allow read from Business Account Insight services"
      },
      "x-ibm-oauth-provider": "access-authorization-1-0-0-8e24"
    },
    "clientId": {
      "description": "ClientId as provided by the Rabobank developer portal.",
      "type": "apiKey",
      "name": "X-IBM-Client-Id",
      "in": "header"
    }
  },
  "definitions": {
    "_linksAccountReport": {
      "type": "object",
      "properties": {
        "account": {
          "type": "string",
          "example": "/accounts/Wp-xhZMGEWRIIgVjPwTC1aKJJ0VCRZ_4bScUVXof7e0",
          "description": "A link to the resource providing the details of one account"
        },
        "next": {
          "type": "string",
          "example": "/accounts/Wp-xhZMGEWRIIgVjPwTC1aKJJ0VCRZ_4bScUVXof7e0/transactions?bookingStatus=booked&dateFrom=2021-01-18T02:41:10.529Z&dateTo=2021-08-18T02:41:10.529Z&size=2&nextPageToken=ckP10o26_WXzc1YYMecNZguB6F-qyncB_64qf_1C6OR2R_EQUZGWsxphe8NclnhH",
          "description": "Navigation link for paginated account reports"
        }
      }
    },
    "accountReference": {
      "type": "object",
      "properties": {
        "bban": {
          "$ref": "#/definitions/bban"
        },
        "currency": {
          "$ref": "#/definitions/currencyCode"
        },
        "iban": {
          "$ref": "#/definitions/iban"
        }
      }
    },
    "accountReport": {
      "type": "object",
      "properties": {
        "_links": {
          "$ref": "#/definitions/_linksAccountReport"
        },
        "booked": {
          "$ref": "#/definitions/transactionList"
        }
      }
    },
    "amount": {
      "type": "object",
      "properties": {
        "value": {
          "$ref": "#/definitions/amountValue"
        },
        "currency": {
          "$ref": "#/definitions/currencyCode"
        }
      }
    },
    "amountValue": {
      "type": "string",
      "pattern": "-?[0-9]{1,14}(\\.[0-9]{1,3})?",
      "description": "The amount given with fractional digits, where fractions must be compliant to the currency definition.\nUp to 14 significant figures. Negative amounts are signed by minus.\nThe decimal separator is a dot.\n**Example:**\nValid representations for EUR with up to two decimals are:\n\n\n\n  * 1056\n\n  * 5768.2\n\n  * -1.50\n\n  * 5877.78\n",
      "example": "6002.17"
    },
    "bban": {
      "type": "string",
      "description": "Basic Bank Account Number (BBAN) Identifier",
      "example": "0125618484"
    },
    "currencyCode": {
      "type": "string",
      "pattern": "[A-Z]{3}",
      "description": "ISO 4217 Alpha 3 currency code\n",
      "example": "EUR"
    },
    "currencyExchange": {
      "type": "object",
      "properties": {
        "sourceCurrency": {
          "$ref": "#/definitions/currencyCode"
        },
        "targetCurrency": {
          "$ref": "#/definitions/currencyCode"
        },
        "exchangeRate": {
          "type": "string",
          "example": "1"
        }
      },
      "description": "Exchange Rate"
    },
    "exchangeRateList": {
      "type": "array",
      "description": "Array of exchange rates",
      "items": {
        "$ref": "#/definitions/currencyExchange"
      }
    },
    "iban": {
      "type": "string",
      "pattern": "[A-Z]{2,2}[0-9]{2,2}[a-zA-Z0-9]{1,30}",
      "description": "IBAN of an account",
      "example": "NL52RABO0125618484"
    },
    "instructedAmountType": {
      "type": "object",
      "properties": {
        "amount": {
          "type": "number",
          "example": 5.12,
          "description": "Instructed Amount"
        },
        "sourceCurrency": {
          "type": "string",
          "example": "EUR",
          "description": "Instructed Amount Currency"
        }
      },
      "description": "Original amount (f.e. USD amount paid towards the EUR DbtrAcct)."
    },
    "balanceAfterBooking": {
      "type": "object",
      "properties": {
        "balanceType": {
          "type": "string",
          "example": "InterimBooked",
          "description": "Type of Balance"
        },
        "balanceAmount": {
          "$ref": "#/definitions/amount"
        }
      },
      "description": "This is the balance of the account after the booked transaction"
    },
    "messageCodeText": {
      "type": "string",
      "example": "CERTIFICATE_INVALID",
      "enum": [
        "SESSIONS_NOT_SUPPORTED",
        "ACCESS_EXCEEDED",
        "REQUESTED_FORMATS_INVALID",
        "CERTIFICATE_INVALID",
        "CERTIFICATE_EXPIRED",
        "CERTIFICATE_BLOCKED",
        "CERTIFICATE_REVOKED",
        "CERTIFICATE_MISSING",
        "SIGNATURE_INVALID",
        "SIGNATURE_MISSING",
        "FORMAT_ERROR",
        "PARAMETER_NOT_SUPPORTED",
        "SERVICE_INVALID",
        "SERVICE_BLOCKED",
        "CORPORATE_ID_INVALID",
        "CONSENT_UNKNOWN",
        "CONSENT_INVALID",
        "CONSENT_EXPIRED",
        "TOKEN_UNKNOWN",
        "TOKEN_INVALID",
        "TOKEN_EXPIRED",
        "RESOURCE_UNKNOWN",
        "RESOURCE_EXPIRED",
        "TIMESTAMP_INVALID",
        "PERIOD_INVALID",
        "SCA_METHOD_UNKNOWN"
      ]
    },
    "purposeCode": {
      "type": "string",
      "description": "ExternalPurposeCode from ISO 20022. Contains the category purpose code, which is the reason for payment transactions.\nList available on http://www.iso20022.org/external_code_list.page\n",
      "example": "EPAY"
    },
    "tppMessageCategory": {
      "type": "string",
      "description": "Category of the message",
      "example": "ERROR",
      "enum": [
        "ERROR",
        "WARNING"
      ]
    },
    "tppMessageGeneric": {
      "type": "object",
      "properties": {
        "category": {
          "$ref": "#/definitions/tppMessageCategory"
        },
        "code": {
          "$ref": "#/definitions/messageCodeText"
        },
        "path": {
          "type": "string"
        },
        "text": {
          "$ref": "#/definitions/tppMessageText"
        }
      }
    },
    "tppMessageText": {
      "type": "string",
      "maxLength": 512,
      "description": "Additional text.",
      "example": "Header X-Request-ID is required"
    },
    "tppMessages": {
      "properties": {
        "tppMessages": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/tppMessageGeneric"
          }
        }
      },
      "description": "Messages on operational issues."
    },
    "transactionDetails": {
      "type": "object",
      "properties": {
        "bookingDate": {
          "type": "string",
          "example": "2021-08-17",
          "description": "Date on which the transaction is booked at the Rabobank in the UTC/GMT timezone. It can contain every date (also weekend and holidays). Book date in format CCYY-MM-DD. C = Century, Y = Year, M = Month, D = Day"
        },
        "creditorAccount": {
          "$ref": "#/definitions/accountReference"
        },
        "creditorAgent": {
          "type": "string",
          "example": "RABONL2UXXX",
          "description": "Creditor Agent BIC."
        },
        "creditorId": {
          "type": "string",
          "example": "NL98ZZZ09999999999",
          "description": "Identification of Creditors, e.g. a SEPA Creditor ID.",
          "maxLength": 35
        },
        "creditorName": {
          "type": "string",
          "example": "Creditor Name",
          "description": "Creditor Name",
          "maxLength": 140
        },
        "debtorAccount": {
          "$ref": "#/definitions/accountReference"
        },
        "debtorAgent": {
          "type": "string",
          "example": "RABONL2UXXX",
          "description": "Debtor Agent BIC."
        },
        "debtorName": {
          "type": "string",
          "example": "Debtor Name",
          "description": "Debtor Name",
          "maxLength": 140
        },
        "endToEndId": {
          "type": "string",
          "example": "90705030",
          "description": "Unique end to end identity.",
          "maxLength": 35
        },
        "entryReference": {
          "type": "string",
          "example": "11900",
          "description": "Sequence number of the booking on a Rabobank account.",
          "maxLength": 35
        },
        "currencyExchange": {
          "$ref": "#/definitions/exchangeRateList"
        },
        "initiatingPartyName": {
          "type": "string",
          "example": "TRX ST",
          "description": "Name of initiating party."
        },
        "instructedAmount": {
          "$ref": "#/definitions/instructedAmountType"
        },
        "mandateId": {
          "type": "string",
          "example": "EEDD-D121-01",
